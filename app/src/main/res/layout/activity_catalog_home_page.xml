                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.717"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.316">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="0dp"
                    android:orientation="horizontal"
                    android:scrollbarThumbHorizontal="@color/cream"
                    android:scrollbarTrackHorizontal="@color/cream">

                    <ImageView
                        android:id="@+id/imageView88"
                        android:layout_width="305dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/continuereadingphoto" />

                    <ImageView
                        android:id="@+id/imageView87"
                        android:layout_width="294dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/building" />
                </LinearLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:id="@+id/HorizontalScrollView9"
                android:layout_width="372dp"
                android:layout_height="230dp"
                android:layout_margin="3dp"
                android:fillViewport="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.740"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.49">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="0dp"
                    android:orientation="horizontal"
                    android:scrollbarThumbHorizontal="@color/cream"
                    android:scrollbarTrackHorizontal="@color/cream">

                    <ImageView
                        android:id="@+id/imageView103"
                        android:layout_width="305dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/featuredresearch" />

                    <ImageView
                        android:id="@+id/imageView104"
                        android:layout_width="305dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/featuredresearch1" />
                    <ImageView
                        android:id="@+id/imageView105"
                        android:layout_width="305dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/featuredresearch2" />


                </LinearLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:id="@+id/HorizontalScrollView10"
                android:layout_width="372dp"
                android:layout_height="200dp"
                android:layout_margin="3dp"
                android:fillViewport="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.740"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.70">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="0dp"
                    android:orientation="horizontal"
                    android:scrollbarThumbHorizontal="@color/cream"
                    android:scrollbarTrackHorizontal="@color/cream">

                    <ImageView
                        android:id="@+id/imageView110"
                        android:layout_width="305dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/greenerbamboo" />

                    <ImageView
                        android:id="@+id/imageView111"
                        android:layout_width="305dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/dause" />
                    <ImageView
                        android:id="@+id/imageView112"
                        android:layout_width="305dp"
                        android:layout_height="wrap_content"
                        android:layout_margin="0dp"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/world" />
                </LinearLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:id="@+id/HorizontalScrollView11"
                android:layout_width="372dp"
                android:layout_height="200dp"
                android:layout_margin="3dp"
                android:fillViewport="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.740"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.93">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_margin="0dp"
