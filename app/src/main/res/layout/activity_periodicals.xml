<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:ignore="MissingDefaultResource">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

    </com.google.android.material.appbar.AppBarLayout>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <!-- Add your scrollable content here -->


            <TextView
                android:id="@+id/textView7"
                android:layout_width="123dp"
                android:layout_height="69dp"
                android:layout_marginTop="460dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="PH plans to create a 19,000 hectares of bamboo plantation"
                android:textAlignment="center"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.025"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="402dp"
                android:layout_height="131dp"
                android:layout_marginBottom="750dp"
                android:background="@drawable/frame"
                android:scaleType="fitXY"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageButton
                android:id="@+id/imageButton16"
                android:layout_width="29dp"
                android:layout_height="30dp"
                android:background="@color/trans"
                android:scaleType="fitXY"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.071"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.057"
                app:srcCompat="@drawable/back_button" />

            <TextView
                android:id="@+id/textView81"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_medium"
                android:text="Periodicals Catalogue"
                android:textColor="#134615"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.508"
                app:layout_constraintStart_toStartOf="@+id/imageView"
                app:layout_constraintTop_toTopOf="@+id/imageView"
                app:layout_constraintVertical_bias="0.51" />

            <TextView
                android:id="@+id/textView1"
                android:layout_width="115dp"
                android:layout_height="73dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="DA: Use bamboo vs flooding"
                android:textAlignment="center"
                android:textSize="13.5sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.055"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="119dp"
                android:layout_height="56dp"
                android:layout_marginTop="152dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="World's first bamboo crash barrier installed on highway in Maharashtra"
                android:textAlignment="center"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.944"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="112dp"
                android:layout_height="59dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Greener bamboo textiles developed in PH"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.479"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="113dp"
                android:layout_height="55dp"
                android:layout_marginTop="236dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Gov't raising P100M for bamboo dev't, promotion in 2024"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.055"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="109dp"
                android:layout_height="59dp"
                android:layout_marginTop="236dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Strategies for Propagule and Shoot Production of Three Bamboo Species"
                android:textAlignment="center"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="128dp"
                android:layout_height="69dp"
                android:layout_marginTop="236dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Rizome invests $100M for engineered bamboo plant in PH"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.952"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="378dp"
                android:layout_height="153dp"
                android:layout_marginTop="484dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.52"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView">

                <ImageButton
                    android:id="@+id/imageview7"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bookpic"
                    android:onClick="phplans" />

                <ImageButton
                    android:id="@+id/imageview8"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bookpic"
                    android:onClick="nestle" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="378dp"
                android:layout_height="153dp"
                android:layout_marginTop="248dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.48"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView">

                <ImageButton
                    android:id="@+id/imageview4"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bookpic"
                    android:onClick="gov" />

                <ImageButton
                    android:id="@+id/imageview5"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bookpic"
                    android:onClick="strategies" />

                <ImageButton
                    android:id="@+id/imageview6"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bookpic"
                    android:onClick="rizome" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="378dp"
                android:layout_height="153dp"
                android:layout_marginTop="24dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.52"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView">

                <ImageButton
                    android:id="@+id/imageview1"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bookpic"
                    android:onClick="da" />

                <ImageButton
                    android:id="@+id/imageview2"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bookpic"
                    android:onClick="greener" />

                <ImageButton
                    android:id="@+id/imageview3"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bookpic"
                    android:onClick="world" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView8"
                android:layout_width="128dp"
                android:layout_height="76dp"
                android:layout_marginTop="460dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Nestle to plant 3.5 million native bamboo clumps and tress in the Philippines"
                android:textAlignment="center"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.516"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>