<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:ignore="MissingDefaultResource">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white">

    </com.google.android.material.appbar.AppBarLayout>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <!-- Add your scrollable content here -->


            <TextView
                android:id="@+id/textView7"
                android:layout_width="123dp"
                android:layout_height="54dp"
                android:layout_marginTop="380dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Bamboo Architecture and Design"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.028"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="402dp"
                android:layout_height="131dp"
                android:layout_marginBottom="610dp"
                android:background="@drawable/frame"
                android:scaleType="fitXY"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageButton
                android:id="@+id/imageButton14"
                android:layout_width="29dp"
                android:layout_height="30dp"
                android:background="@color/trans"
                android:scaleType="fitXY"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.07"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.068"
                app:srcCompat="@drawable/back_button" />

            <TextView
                android:id="@+id/textView81"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppins_medium"
                android:text="Book Listing Catalogue"
                android:textColor="#134615"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.508"
                app:layout_constraintStart_toStartOf="@+id/imageView"
                app:layout_constraintTop_toTopOf="@+id/imageView"
                app:layout_constraintVertical_bias="0.51" />

            <LinearLayout
                android:id="@+id/linearLayout"
                android:layout_width="378dp"
                android:layout_height="153dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.515"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView">

                <ImageButton
                    android:id="@+id/imageview1"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/howtobuildwithbamboo"
                    android:onClick="howtobuildwithbamboo" />

                <ImageButton
                    android:id="@+id/imageview2"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/engineeredbamboostructures"
                    android:onClick="engineeredbamboostructures" />

                <ImageButton
                    android:id="@+id/imageview3"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/illustrationsofbamboosinchina"
                    android:onClick="illustrationsofbamboos" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView1"
                android:layout_width="107dp"
                android:layout_height="38dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="How To Build With Bamboo"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.055"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="123dp"
                android:layout_height="33dp"
                android:layout_marginTop="152dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Illustrations of Bamboos in China"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.947"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="121dp"
                android:layout_height="39dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Engineered Bamboo Structures"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.479"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <LinearLayout
                android:id="@+id/linearLayout2"
                android:layout_width="378dp"
                android:layout_height="153dp"
                android:layout_marginTop="192dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.515"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView">

                <ImageButton
                    android:id="@+id/imageview4"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bamboogridshells"
                    android:onClick="bamboogridshells" />

                <ImageButton
                    android:id="@+id/imageview5"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bambooinvietnam"
                    android:onClick="bambooinvietnam" />

                <ImageButton
                    android:id="@+id/imageview6"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/farmingbamboo"
                    android:onClick="farmingbamboo" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView4"
                android:layout_width="114dp"
                android:layout_height="35dp"
                android:layout_marginTop="192dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Bamboo Gridshells"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.039"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="110dp"
                android:layout_height="41dp"
                android:layout_marginTop="192dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Bamboo in Vietnam"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="86dp"
                android:layout_height="45dp"
                android:layout_marginTop="192dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Farming Bamboo"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.916"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="378dp"
                android:layout_height="153dp"
                android:layout_marginTop="380dp"
                android:orientation="horizontal"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.515"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView">

                <ImageButton
                    android:id="@+id/imageview7"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/bambooarchitectureanddesign"
                    android:onClick="bambooarchitecture" />

                <ImageButton
                    android:id="@+id/imageview8"
                    android:layout_width="106dp"
                    android:layout_height="146dp"
                    android:layout_margin="10dp"
                    android:background="@drawable/buildingwithbamboo"
                    android:onClick="buildingwithbamboo" />

            </LinearLayout>

            <TextView
                android:id="@+id/textView8"
                android:layout_width="128dp"
                android:layout_height="76dp"
                android:layout_marginTop="384dp"
                android:fontFamily="@font/poppins_semibold"
                android:text="Building With Bamboo: A Handbook 2nd Edition"
                android:textAlignment="center"
                android:textSize="12sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>